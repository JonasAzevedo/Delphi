  object qryItens: TSQLQuery
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      
        'SELECT i.codigo,i.quantidade,i.valor,p.nome AS "PRODUTO",p.codig' +
        'o AS "COD_PRODUTO"'
      'FROM venda v, item_venda i, produto p'
      'WHERE i.cod_venda = v.codigo'
      'AND i.cod_produto = p.codigo'
      'AND v.codigo = 1')
    SQLConnection = DM.SQLConnection
    Left = 136
    Top = 253
    object qryItensCODIGO: TIntegerField
      FieldName = 'CODIGO'
      Required = True
