  object qryLocacaoResumo: TSQLQuery
    MaxBlobSize = -1
    Params = <>
    SQL.Strings = (
      
        'SELECT l.codigo,l.data_inicio,l.data_final,l.sexo,l.observacoes,' +
        'l.valor_total,l.valor_pago,'
      
        '       (l.valor_total-l.valor_pago) AS "Valor_Resta",l.status,c.' +
        'nome'
      'FROM locacao l, cliente c'
      'WHERE l.cod_cliente=c.codigo'
      'ORDER BY l.data_inicio')
    SQLConnection = DM.SQLConnection
    Left = 47
    Top = 94
