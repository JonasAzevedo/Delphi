  object zQryImovelInteresse: TZQuery
    Connection = DM.ZConnection
    SQL.Strings = (
      'SELECT ii.codigo AS codImovInteresse,ii.ativo,'
      '       CAST(ii.data_cadastro AS DATE) AS data_cadastro,'
      '       ii.data_captacao,ii.data_baixa,'
      '       ii.tipo_pessoa,'
      '       CASE WHEN ii.tipo_pessoa ='#39'F'#205'SICA'#39' THEN pf.nome'
      '            WHEN ii.tipo_pessoa = '#39'JUR'#205'DICA'#39' THEN pj.nome'
      '            ELSE '#39'NENHUMA'#39' END AS NOME_TIPO_PESSOA,'
      '       t.nome AS nomeTipoImovel,'
      '       ii.finalidade_imovel,ii.imovel_desejavel,'
      '       ii.valor_imovel,ii.condicao_pagamento,'
      
        '       ii.pgto_vista_cach,ii.pgto_vista_valor_bem,ii.pgto_vista_' +
        'descricao_bem,'
      
        '       ii.pgto_fincto_cach,ii.pgto_fincto_cach_financiado,ii.pgt' +
        'o_fincto_valor_bem,ii.pgto_fincto_descricao_bem,'
      '       ii.observacao,ii.caracteristicas'
      'FROM imovel_de_interesse ii'
      'JOIN tipo_imovel t ON ii.cod_fk_tipo_imovel=t.codigo'
      
        'LEFT JOIN agenc_pessoa_fisica pf ON ii.cod_fk_cliente_pessoa_fis' +
        'ica = pf.codigo'
      
        'LEFT JOIN agenc_pessoa_juridica pj ON ii.cod_fk_cliente_pessoa_j' +
        'uridica = pj.codigo'
      'WHERE 1=2')
    Params = <>
    Left = 714
    Top = 8
    object zQryImovelInteresseCODIMOVINTERESSE: TIntegerField
