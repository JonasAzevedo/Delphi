  object zQryRegiaoLocalidade: TZQuery
    Connection = DM.ZConnection
    SQL.Strings = (
      
        'SELECT iirl.codigo,iirl.cod_fk_imovel_de_interesse,iirl.cod_fk_r' +
        'egiao_localidade,'
      '       rl.nome AS nomeRegiaoLocalidade'
      'FROM imov_interesse_regiao_local iirl'
      
        'JOIN regiao_localidade rl ON iirl.cod_fk_regiao_localidade=rl.co' +
        'digo'
      'WHERE iirl.cod_fk_imovel_de_interesse=1'
      'AND 1=2')
    Params = <>
    Left = 742
    Top = 8
    object zQryRegiaoLocalidadeCODIGO: TIntegerField
      FieldName = 'CODIGO'
      Required = True
    end
    object zQryRegiaoLocalidadeCOD_FK_IMOVEL_DE_INTERESSE: TIntegerField
      FieldName = 'COD_FK_IMOVEL_DE_INTERESSE'
      Required = True
    end
    object zQryRegiaoLocalidadeCOD_FK_REGIAO_LOCALIDADE: TIntegerField
      FieldName = 'COD_FK_REGIAO_LOCALIDADE'
      Required = True
    end
    object zQryRegiaoLocalidadeNOMEREGIAOLOCALIDADE: TStringField
      FieldName = 'NOMEREGIAOLOCALIDADE'
      Required = True
      Size = 50
    end
  end
